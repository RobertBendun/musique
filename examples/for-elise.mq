oct 5; bpm 72; len (1/16);

subsection1 := (
  sim (a 4 en) (a 2 e 3 a 3);
  play (oct 4; c e a);

  sim (b 4 en) (e 2 e 3 g# 3);
  play (oct 4; e g# b);

  sim (c 5 en) (a 2 e 3 a 3);
  play (e 4 e 5 d# 5);

  play (e d# e b 4 d c);

  sim (a 4 en) (a 2 e 3 a 3);
  play (c 4 e 4 a 4);

  sim (b 4 en) (e 2 e 3 g# 3);
  play (d 4 c 5 b 4);
);

section1 := ( n |
  play e d#;
  play (e d# e b 4 d c);

  call subsection1;

  if (n == 1)
    ( sim (a 4 qn) (a 2 e 3 a 3) )
    ( sim (a 4 en) (a 2 e 3 a 3)
    ; play (b 4 c 5 d 5)
    )
);

section2 := ( n |
  sim (e 5 den) (c 3 g 3 c 4);
  play (g 4 f e);

  sim (d 5 den) (g 2 g 3 b 4);
  play (f 4 e d);

  sim (c 5 den) (a 2 e 3 a 3);
  play (e 4 d c);

  sim (b 4 en) (e 2 e 3 e 4);
  play (e 4 e 5 e 4 e 4 e 5 e 6 d# 5 e 5 d# 5 e 5 en);

  play (d# e d# e d#);
  play (e d# e b 4 d c);

  call subsection1;

  if (n == 1)
    ( sim (a 4 en) (a 2 e 3 a 3)
    ; play (b 4 c 5 d 5)
    )
);

section1 1;
section1 2;
section2 1;

